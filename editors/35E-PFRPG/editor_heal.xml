<?xml version="1.0" encoding="iso-8859-1"?>

<root>
    <windowclass name="editor_heal">
        <script file="editors/35E-PFRPG/scripts/editor_heal.lua" />
        <sizelimits>
            <minimum height="25" />
        </sizelimits>
        <sheetdata>
            <leftanchor />

            <stringcycler_effects name="effect">
                <parameters>
                    <defaultlabelres mergerule="replace">heal</defaultlabelres>
                    <defaultvalue>HEAL</defaultvalue>
                    <labelsres>fast_heal|regen</labelsres>
                    <values>FHEAL|REGEN</values>
                </parameters>
            </stringcycler_effects>

            <effect_number name="effect_modifier" />

            <combobox_bonus_types name="effect_bonus_type" />
        </sheetdata>
    </windowclass>
</root>
